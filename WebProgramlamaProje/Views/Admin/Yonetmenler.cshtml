@model IEnumerable<WebProgramlamaProje.Models.Yonetmen>

@{
    ViewData["Title"] = "Yonetmenler";
}

<h1>Yonetmenler</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.YonetmenAd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YonetmenYas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YonetmenDesc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.YonetmenAd)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.YonetmenYas)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.YonetmenDesc)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.YonetmenID">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.YonetmenID">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.YonetmenID">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
